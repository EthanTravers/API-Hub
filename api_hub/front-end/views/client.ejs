<%- include('header.ejs'); -%>
<div id="app">
    <%- include('navbar'); -%>
    <div v-if="page == 'home'">
        <%- include('home'); -%>
    </div>
    <div v-else-if="page =='profile'">
        <div v-if="user.isLoggedIn === false">
            <%- include('login'); -%>
        </div>
        <div v-if="user.isLoggedIn === true">
            <h1>You are logged in!</h1>
        </div>
    </div>
    <div v-else-if="page =='exploreAPI'">
        <div v-if="user.isLoggedIn === false">
            <h1>You must be logged in to do that!</h1>
        </div>
        <div v-else-if="user.isLoggedIn === true && user.explored === false">
            <%- include('submit_API_files'); -%>
        </div>
        <div v-else-if="user.isLoggedIn === true && user.explored === true">
            <%- include('show_explored_API'); -%>
        </div>
    </div>
</div>
<%- include('footer.ejs'); -%>