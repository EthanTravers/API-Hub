<div class="login-card">
  <form>
    <div class="form-group">
      <label for="login-username">Username</label>
      <input type="text" placeholder="Username" autocomplete="on" v-model="username" id="login-username"/>
    </div>
    <div class="form-group">
      <label for="login-password">Password</label>
      <input type="password" placeholder="Password" autocomplete="on" v-model="password" id="login-password"/>
    </div>
    <button @click="Register(username, password)" type="submit">Register</button>
    <button @click="login(username, password)" type="submit">Login</button>
  </form>
</div>
